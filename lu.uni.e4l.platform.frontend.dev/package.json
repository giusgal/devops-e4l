{
  "name": "e4l",
  "version": "1.0.0",
  "description": "Energy4Life",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env PUBLIC_PATH=PUBLIC_PATH/ --mode production",
    "start": "webpack serve --host 0.0.0.0 --open --env PUBLIC_PATH=/ --mode development",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "<rootDir>/src/tests/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/src/tests/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/src/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/js/**/*.js",
      "!src/js/index.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "cobertura"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "babel-loader": "^9.1.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "redux-mock-store": "^1.5.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-combine-loaders": "^2.0.4",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "sass-loader": "^13.3.2",
    "sass": "^1.69.5",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.4",
    "dotenv-webpack": "^8.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "terser-webpack-plugin": "^5.3.9",
    "copy-webpack-plugin": "^11.0.0",
    "file-loader": "^6.2.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.14.18",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "redux-promise-middleware": "^6.2.0",
    "redux-logger": "^3.0.6",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@xstate/react": "^3.2.2",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "react-bootstrap": "^2.9.1",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-table": "^7.8.0",
    "reactstrap": "^9.2.1",
    "framer-motion": "^10.16.16",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.20.1",
    "react-select": "^5.8.0",
    "react-datepicker": "^4.21.0",
    "react-countup": "^6.5.0",
    "react-i18next": "^13.5.0",
    "react-tabs": "^6.0.2",
    "react-switch": "^7.0.0",
    "react-window": "^1.8.10",
    "react-virtualized": "^9.22.5",
    "react-google-recaptcha": "^3.1.0",
    "react-chartjs-2": "^5.2.0",
    "react-textfit": "^1.1.1",
    "chart.js": "^4.4.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "chartjs-plugin-piechart-outlabels": "^0.1.4",
    "mdbreact": "^5.2.0",
    "tabler-react": "^1.0.0-alpha.11",
    "i18next": "^23.7.8",
    "i18next-browser-languagedetector": "^7.2.0",
